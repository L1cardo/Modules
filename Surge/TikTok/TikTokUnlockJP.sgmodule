#!name=TikTok解锁 日本
#!desc=解除 TikTok 区域限制 日本 - by Licardo
#!system=ios

[URL Rewrite]
(?<=_region=)CN(?=&) url 307 JP
(?<=&app_version=)1[6-9]..(?=.?.?&) url 307 1
(?<=\?version_code=)1[6-9]..(?=.?.?&) url 307 1

[MITM]
hostname = %APPEND% *.tiktokv.com, *.byteoversea.com, *.musical.ly, *.snssdk.com
